{
  "name": "vite-plugin-svg-sprite",
  "version": "0.6.1",
  "description": "SVG sprite plugin for Vite",
  "type": "module",
  "exports": {
    ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
    },
    "./runtime/*.js": {
      "types": "./dist/runtime/*.d.ts",
      "import": "./dist/runtime/*.js"
    },
    "./typings/*": {
      "types": "./typings/*.d.ts"
    }
  },
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "typings",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint src",
    "build": "shx rm -rf cjs esm && rolldown",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite",
    "svg"
  ],
  "author": "meowtec<bertonzh@gmail.com>",
  "homepage": "https://github.com/meowtec/vite-plugin-svg-sprite",
  "bugs": {
    "url": "https://github.com/meowtec/vite-plugin-svg-sprite/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/micromatch": "^4.0.1",
    "@types/node": "^20.4.2",
    "@types/react": "^18.3.12",
    "eslint": "^9.17.0",
    "rolldown": "1.0.0-beta.1",
    "shx": "^0.3.4",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.3",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.9.4",
    "micromatch": "^4.0.2",
    "svgo": "^3.0.2"
  },
  "peerDependencies": {
    "react": "17 || 18 || 19",
    "vite": "2 || 3 || 4 || 5 || 6",
    "vue": "3"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}
